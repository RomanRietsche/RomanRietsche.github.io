@misc{hilbert.2015.archetypes,
  title = {The Archetypes of Job Roles for Instructional Designers with a Career in Higher Education},
  author = {Hilbert, Erick T.},
  year = {2015},
  month = jan,
  journal = {ProQuest Dissertations and Theses},
  abstract = {Previous researchers have identified desirable characteristics of instructional designers, but very little research has been published on the archetypes or models of these characteristics when compared to job roles instructional designers fill in higher education. The purpose of this sequential exploratory mixed methods study was to identify the job roles of instructional designers in higher education and to define archetypes for the identified job roles. The sequential exploratory mixed methods study added to the field of instructional design (ID) by identifying the job roles of instructional designers in higher education, utilizing the Repertory Grid Technique (RGIP) to identify desirable characteristics of instructional designers, and rate categories of the characteristics of the previous identified job roles. Research participants were instructional designers with at least three years of higher education ID experience. As a result of the study eight job roles instructional designers fill in higher education were identified, 14 categories of desirable characteristics were defined, and these categories were ranked and rated to determine their importance to each job role. The researcher confirmed archetypes for ID jobs in higher education exist. The results of the study are useful in the formal education, the hiring, and evaluation of job performance of instructional designers, and as another tool to measure the quality of online education. Additional research is recommended to determine how well instructional design education aligns to instructional design job roles and how much better do instructional designers perform when they possess the correct characteristics for their job role.},
  isbn = {9781339227108},
  keywords = {0447:Instructional Design,0710:Educational technology,Characteristics,EDUCATION,EDUCATIONAL technology,Instructional design,Job roles,Repertory}
