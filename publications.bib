@article{motta.2020.quantum,
  title = {Quantum Simulation of Electronic Structure with a Transcorrelated {{Hamiltonian}}: Improved Accuracy with a Smaller Footprint on the Quantum Computer},
  author = {Motta, Mario and Gujarati, Tanvi P. and Rice, Julia E. and Kumar, Ashutosh and Masteran, Conner and Latone, Joseph A. and Lee, Eunseok and Valeev, Edward F. and Takeshita, Tyler Y.},
