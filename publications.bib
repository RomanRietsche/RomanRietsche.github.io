@misc{manning.2024.automated,
  title = {Automated {{Social Science}}: {{Language Models}} as {{Scientist}} and {{Subjects}}},
  shorttitle = {Automated {{Social Science}}},
  author = {Manning, Benjamin S. and Zhu, Kehang and Horton, John J.},
  year = {2024},
