@misc{butori.2012.exploring,
  title = {Exploring Best Practices for Designing Outcomes-Based Performance Assessment: {{A Delphi}} Study},
  author = {Butori, Theresa A.},
  year = {2012},
  month = jan,
  journal = {ProQuest Dissertations and Theses},
  abstract = {The purpose of this study was to compile best practices that instructional design experts implement when designing outcomes-based performance assessments and to explore instructional design processes that lead to reliable and effective performance assessments for learners. The research problem identified that there is a need to determine what constitutes effective best practices in designing performance assessments and to determine consensus on instructional design elements that are necessary for designing performance assessments. A pilot study was conducted with 5 expert participants to validate the instrument used in the first round of the study. The Delphi Technique was implemented to collect information from a panel of 12 instructional design experts in higher education. The findings in this research study determined ten best practices and instructional design elements summarized as defining the outcomes that would demonstrate competency; perform a complete cognitive analysis of the assessment; assessment methods should align with instructional context, techniques, and goals; an assessment task must be a direct measure of the outcome; the task should be clear, precise, and measureable; performance requires application and other higher order thinking skills; tasks should be contextual or relevant to the real world; criteria for evaluation should be explicitly explained; criteria can be clarified using work examples; consider learner characteristics, prior knowledge, and skills for the course; analyze targeted learner needs for the learning; offer learners with choices or allow personalized solutions; and, after the performance, learners are provided with descriptive feedback. Among the findings were that several panel members implemented course design models focused on outcomes, performance assessment, criteria, and feedback as intended during their design process. Consensus was achieved in all areas of this research study, which may prove useful to the instructional design profession. Furthermore, the course design process detailed in the themes of the study is indicative of a shift in the use of a constructivist approach to the design of assessments. The findings in this research study suggest that experts in instructional design support a constructivist approach to assessment, which is evident in the design process for assessments.},
  isbn = {9781267259738},
  keywords = {0443:Educational evaluation,Best practices,EDUCATION,Educational evaluation,Instructional design,Outcomes-based assessment,Performance assessment}
}

